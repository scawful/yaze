# Local signing defaults for iOS builds (DO NOT COMMIT SECRETS).
#
# Copy to scripts/signing.env (gitignored) and edit.

# Use your personal team for local device builds.
export YAZE_IOS_TEAM_ID="YOUR_TEAM_ID"

# Use a bundle ID you control under that team.
export YAZE_IOS_BUNDLE_ID="com.yourcompany.yaze-ios"

# iCloud container to match your bundle ID (must exist + be enabled on the App ID).
export YAZE_ICLOUD_CONTAINER_ID="iCloud.${YAZE_IOS_BUNDLE_ID}"

# If xcodebuild can't use Xcode Accounts (headless/CI), use an App Store Connect key:
# export XCODE_AUTH_KEY_PATH="$HOME/path/to/AuthKey_XXXXXX.p8"
# export XCODE_AUTH_KEY_ID="XXXXXX"
# export XCODE_AUTH_KEY_ISSUER_ID="XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX"
